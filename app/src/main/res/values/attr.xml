<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AddItemView">
        <attr name="aivtitle" format="string" />
        <attr name="aivdesc" format="string" />
        <attr name="aivsrc" format="reference" />
    </declare-styleable>


    <declare-styleable name="FontFamilyFont">
        <attr name="fontStyle">
            <enum name="normal" value="0" />
            <enum name="italic" value="1" />
        </attr>
        <attr name="font" format="reference" />
        <attr name="fontWeight" format="integer" />
        <attr name="fontVariationSettings" format="string" />
        <attr name="ttcIndex" format="integer" />
        <attr name="android:fontStyle" />
        <attr name="android:font" />
        <attr name="android:fontWeight" />
        <attr name="android:fontVariationSettings" />
        <attr name="android:ttcIndex" />
    </declare-styleable>

</resources>